<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="github" value="https://nuget.pkg.github.com/<where-your-packages-are-username-or-organization-name>/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <github>
      <add key="Username" value="<your-github-username>" />
      <add key="ClearTextPassword" value="<your-generated-personal-access-token>" />
    </github>
  </packageSourceCredentials>
</configuration>
